{
  "name": "express_api",
  "version": "0.0.1",
  "description": "api based on express and mongoose",
  "main": "app.js",
  "scripts": {
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "dev": "DEBUG=api* NODE_ENV=development nodemon --exec babel-node ./src/index.js",
    "w-dev": "set DEBUG=api* && set NODE_ENV=development && nodemon --exec babel-node ./src/index.js",
    "start": "NODE_ENV=production node ./build/index.js",
    "lint": "npx standard",
    "clean": "rm -rf build && mkdir build"
  },
  "author": "Guillermo O. Anselmi",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.464.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "cloudinary": "^1.14.0",
    "connect-multiparty": "^2.2.0",
    "cookie-parser": "^1.4.3",
    "debug": "^4.0.1",
    "dotenv": "^8.0.0",
    "dotenv-flow": "^0.4.0",
    "eslint-config-standard": "^12.0.0",
    "express": "^4.16.3",
    "glob": "^7.1.4",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "moment": "^2.29.2",
    "mongoose": "^5.2.17",
    "mongoose-unique-validator": "^2.0.2",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "node-pushnotifications": "^1.1.7",
    "nodemailer": "^6.2.1",
    "spherical-geometry-js": "^1.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "nodemon": "^1.19.1",
    "standard": "^14.3.1"
  }
}
